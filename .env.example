# Claude API Key (used by Ring 1 evolution engine)
CLAUDE_API_KEY=

# DashScope API Key (for Qwen / 千问 models)
DASHSCOPE_API_KEY=

# Per-machine LLM provider override (takes precedence over config.toml)
# Supported: anthropic, openai, deepseek, qwen, minimax, kimi, gemini
#
# --- Qwen (千问) ---
# LLM_PROVIDER=qwen
# LLM_API_KEY_ENV=DASHSCOPE_API_KEY
# LLM_MODEL=qwen3.5-plus
# LLM_MAX_TOKENS=8192
#
# --- MiniMax ---
# LLM_PROVIDER=minimax
# LLM_API_KEY_ENV=MINIMAX_API_KEY
# LLM_MODEL=MiniMax-Text-01
# LLM_MAX_TOKENS=8192
#
# --- Kimi (Moonshot) ---
# LLM_PROVIDER=kimi
# LLM_API_KEY_ENV=MOONSHOT_API_KEY
# LLM_MODEL=moonshot-v1-8k
# LLM_MAX_TOKENS=8192
#
# --- Gemini ---
# LLM_PROVIDER=gemini
# LLM_API_KEY_ENV=GEMINI_API_KEY
# LLM_MODEL=gemini-2.0-flash
# LLM_MAX_TOKENS=8192
#
# --- Ollama (local) ---
# LLM_PROVIDER=ollama
# LLM_MODEL=llama3
# LLM_API_URL=http://localhost:11434/v1/chat/completions
# LLM_MAX_TOKENS=8192

# Telegram Bot Token
TELEGRAM_BOT_TOKEN=

# Telegram Chat ID (auto-detected from first message if empty)
TELEGRAM_CHAT_ID=
